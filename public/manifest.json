{
	"name": "Youtube-note",
	"description": "A note can add on youtube page",
	"version": "1.1",
	"manifest_version": 3,
	"permissions": ["activeTab", "scripting"],
	"action": {
		"default_icon": "icon-48.png",
		"default_popup": "index.html",
		"default_title": "Youtube-note"
	},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"host_permissions": ["https://www.youtube.com/"]
}
